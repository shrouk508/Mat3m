@model Supply
<section id="contact" class="contact">

    <div class="container" data-aos="fade-up">

        <header class="section-header">
            <h2> التواصل </h2>
            <p>
                سجل الأن
            </p>
        </header>

        <div class="row">

            <div class="col-lg-12">


            </div>

            <div class="col-lg-6">
                <form asp-action="Save_Supplied" method="post">
                    <div class="row gy-4">

                        <div class="col-md-6">
                            <input type="text" asp-for="FName" class="form-control" placeholder=" الاسم الاول" required>
                            <span asp-validation-for="FName" class="text-danger"></span>
                        </div>

                        <div class="col-md-6">
                            <input type="text" asp-for="LName" class="form-control" placeholder=" الاسم الاخير" required>
                            <span asp-validation-for="LName" class="text-danger"></span>
                        </div>

                        <div class="col-md-12">
                            <input type="text" class="form-control" asp-for="Address" placeholder="العنوان" required>
                            <span asp-validation-for="Address" class="text-danger"></span>
                        </div>
                        <div class="col-md-12">
                            <input type="text" class="form-control" asp-for="NId" placeholder="الرقم القومي" required>
                            <span asp-validation-for="NId" class="text-danger"></span>
                        </div>
                        <div class="col-md-12">
                            <input type="text" class="form-control" asp-for="Phone" placeholder="الهاتف" required>
                            <span asp-validation-for="Phone" class="text-danger"></span>
                        </div>
                       




                        <div class="col-md-12 text-center">
                            <button type="submit"class="btn btn-info" style="padding: 10px 30px; color: #ffffff "><strong>التسجيل</strong></button>
                        </div>

                    </div>
                </form>

            </div>

        </div>

    </div>

</section><!-- End Contact Section -->
